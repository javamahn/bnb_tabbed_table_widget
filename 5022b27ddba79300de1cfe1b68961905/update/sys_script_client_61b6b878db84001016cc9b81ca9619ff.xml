        <name>onChange - show/hide data table options</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading) {
        return;
    }
	
	var showSupportedOptions = newValue == g_scratchpad.customTableWidgetId;
	if (showSupportedOptions) {
		g_scratchpad.canSupportTableActions = true;
		g_form.setSectionDisplay('search', true);
		g_form.showRelatedList('x_71627_tab_table_action');
	} else {
		g_scratchpad.canSupportTableActions = false;
		g_form.setSectionDisplay('search', false);
		g_form.hideRelatedList('x_71627_tab_table_action');
	}

}]]></script>
